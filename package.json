{
  "name": "ko-component-router",
  "version": "2.0.0",
  "description": "Component-based routing for KnockoutJS",
  "main": "ko-component-router.js",
  "scripts": {
    "build": "browserify src/index.js -s ko-component-router -x knockout -o ko-component-router.js && uglifyjs ko-component-router.js -o ko-component-router.min.js",
    "watch": "watchify src/index.js -s ko-component-router -x knockout -o ko-component-router.js",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS"
  },
  "keywords": [
    "ko",
    "knockout",
    "component",
    "router"
  ],
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "knockout": "^3.3.0"
  },
  "dependencies": {
    "page": "^1.6.3"
  },
  "jshintConfig": {
    "asi": true,
    "node": true,
    "browser": true
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.2.0",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "sinon": "^1.15.4",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  }
}
