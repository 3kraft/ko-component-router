{
  "name": "ko-component-router",
  "version": "4.0.0-pre.5",
  "description": "Component-based routing for KnockoutJS",
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "homepage": "https://github.com/Profiscience/ko-component-router",
  "bugs": "https://github.com/Profiscience/ko-component-router/issues",
  "repository": "Profiscience/ko-component-router",
  "main": "dist/ko-component-router.js",
  "scripts": {
    "build": "concurrently -p name --names \"DIST,LIB\" -c blue,cyan \"webpack\" \"babel src -d lib\"",
    "watch": "concurrently --kill-others -p name --names \"DIST,LIB\" -c blue,cyan \"webpack --watch\" \"babel src -wd lib\"",
    "lint": "eslint --max-warnings=0 *.js",
    "format": "eslint --fix --max-warnings=0 *.js",
    "test": "karma start",
    "test:watch": "karma start karma.watch.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "knockoutjs",
    "knockout",
    "ko",
    "component",
    "router",
    "routing",
    "spa"
  ],
  "dependencies": {
    "path-to-regexp": "^1.2.1"
  },
  "peerDependencies": {
    "knockout": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-functions": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-runtime": "^6.9.2",
    "concurrently": "^3.0.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.0.1",
    "eslint-config-profiscience": "^1.0.1",
    "isparta-loader": "^2.0.0",
    "jquery": "^3.1.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-tap": "^3.0.1",
    "karma-webpack": "^1.7.0",
    "knockout": "^3.3.0",
    "lodash": "^4.17.2",
    "tape": "^4.4.0",
    "webpack": "^1.11.0"
  }
}
