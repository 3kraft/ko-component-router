{
  "name": "ko-component-router",
  "version": "4.4.4",
  "description": "Component-based routing for KnockoutJS",
  "author": "Casey Webb <notcaseywebb@gmail.com>",
  "license": "WTFPL",
  "homepage": "https://github.com/Profiscience/ko-component-router",
  "bugs": "https://github.com/Profiscience/ko-component-router/issues",
  "repository": "Profiscience/ko-component-router",
  "main": "dist/umd/index.js",
  "jsnext:main": "dist/modules/index.js",
  "module": "dist/modules/index.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-umd": "tsc --declaration true --declarationDir dist/typings --module umd --outDir dist/umd",
    "build-modules": "tsc --module es2015 --outDir dist/modules",
    "_build": "npm run build-umd && npm run build-modules",
    "build": "fly build",
    "watch": "fly watch",
    "test": "fly test",
    "test:watch": "fly test:watch",
    "lint": "eslint --max-warnings=0 src",
    "start": "webpack-dev-server --config examples/webpack.config.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "knockoutjs",
    "knockout",
    "ko",
    "component",
    "router",
    "routing",
    "spa"
  ],
  "dependencies": {
    "is-plain-object": "^2.0.1",
    "path-to-regexp": "^1.2.1"
  },
  "peerDependencies": {
    "knockout": "^3.3.0"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.40",
    "@types/knockout": "^3.4.39",
    "@types/lodash": "^4.14.54",
    "@types/node": "^7.0.5",
    "@types/path-to-regexp": "^1.0.29",
    "@types/socket.io": "^1.4.28",
    "@types/tape": "^4.2.29",
    "awesome-typescript-loader": "^3.0.8",
    "body-parser": "^1.17.0",
    "chalk": "^1.1.3",
    "connect": "^3.6.0",
    "coveralls": "^2.11.9",
    "cross-spawn": "^5.1.0",
    "eslint": "^3.16.1",
    "eslint-config-profiscience": "^1.3.1",
    "fly": "^2.0.5",
    "fly-concat": "^1.2.0",
    "fly-rollup": "^2.0.0",
    "fly-uglify": "^2.1.1",
    "fly-watch": "^1.1.1",
    "fly-webpack": "^1.1.1",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "knockout": "^3.3.0",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.4",
    "minimist": "^1.2.0",
    "opn": "^4.0.2",
    "remap-istanbul": "^0.9.1",
    "rollup": "^0.41.1",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-ts": "^0.1.0",
    "serve-static": "^1.12.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "tap-parser": "^5.3.3",
    "tape": "caseyWebb/tape",
    "tslib": "^1.6.0",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
